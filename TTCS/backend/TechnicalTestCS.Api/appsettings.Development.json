{
  "ConnectionStrings": {
    "Db": "Host=localhost;Port=5432;Database=technicaltestcs;Username=postgres;Password=postgres"
  },
  "ExternalApi": { "BaseUrl": "https://cms-beta.happytal.com" },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  }
}
